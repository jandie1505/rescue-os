{
  "policies": {
    "SearchEngines": { "Default": "DuckDuckGo" },
    "SearchSuggestEnabled": false,
    "PromptForDownloadLocation": true,
    "OfferToSaveLoginsDefault": false,

    "ExtensionSettings": {
      "*": { "installation_mode": "allowed" },
      "uBlock0@raymondhill.net": {
        "installation_mode": "normal_installed",
        "install_url": "https://addons.mozilla.org/firefox/downloads/latest/ublock-origin/latest.xpi",
        "private_browsing": true
      }
    },

    "Preferences": {
      "browser.newtabpage.activity-stream.showSearch": { "Value": true,  "Status": "default" },
      "browser.newtabpage.activity-stream.feeds.topsites": { "Value": false, "Status": "default" },
      "browser.newtabpage.activity-stream.showSponsoredTopSites": { "Value": false, "Status": "default" },
      "browser.newtabpage.activity-stream.feeds.section.highlights": { "Value": false, "Status": "default" },
      "browser.newtabpage.activity-stream.feeds.snippets": { "Value": false, "Status": "default" },
      "browser.newtabpage.activity-stream.feeds.system.topsites": { "Value": false, "Status": "default" },
      "browser.newtabpage.activity-stream.feeds.section.topstories": { "Value": false, "Status": "default" },

      "browser.download.useDownloadDir": { "Value": false, "Status": "default" },
      "browser.download.always_ask_before_handling_new_types": { "Value": true, "Status": "default" },

      "browser.search.suggest.enabled": { "Value": false, "Status": "default" },
      "browser.urlbar.suggest.searches": { "Value": false, "Status": "default" },

      "browser.contentblocking.category": { "Value": "strict", "Status": "default" },

      "privacy.donottrackheader.enabled": { "Value": true, "Status": "default" },
      "privacy.globalprivacycontrol.enabled": { "Value": true, "Status": "default" },

      "privacy.sanitize.sanitizeOnShutdown": { "Value": true, "Status": "default" },
      "privacy.clearOnShutdown.cookies": { "Value": true, "Status": "default" },
      "network.cookie.lifetimePolicy": { "Value": 2, "Status": "default" },

      "signon.rememberSignons": { "Value": false, "Status": "default" },
      "extensions.formautofill.creditCards.enabled": { "Value": false, "Status": "default" },

      "browser.privatebrowsing.autostart": { "Value": true, "Status": "default" },

      "datareporting.healthreport.uploadEnabled": { "Value": false, "Status": "default" },
      "toolkit.telemetry.unified": { "Value": false, "Status": "default" },
      "toolkit.telemetry.enabled": { "Value": false, "Status": "default" },
      "app.shield.optoutstudies.enabled": { "Value": false, "Status": "default" },
      "browser.tabs.crashReporting.sendReport": { "Value": false, "Status": "default" },

      "dom.security.https_only_mode": { "Value": true, "Status": "default" }
    }
  }
}

